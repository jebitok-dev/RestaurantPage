{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "import/prefer-default-export": "off",
      "import/extensions": [".js", ".jsx", ".json", ".ts", ".tsx"],
      "eol-last": "off",
      "no-unused-vars": "off"
    },
    "ignorePatterns": [
      "dist/",
      "build/"
    ]
  }