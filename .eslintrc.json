{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": ["airbnb-base"],
    "rules": {
      "no-shadow": "off",
      "no-param-reassign": "off",
      "import/prefer-default-export": "off",
      "eol-last": "off",
      "no-unused-vars": "off",
        "import/prefer-default-export": "off",
        "no-use-before-define": ["off", { "functions": true, "classes": true }],
        "import/extensions": [
        "error",
        "ignorePackages",
        {
        "js": "never",
        "jsx": "never",
        "json": "never",
        "ts": "never",
        "tsx": "never"
        }
        ],
        "import/no-named-default": "off"
    },
    "ignorePatterns": [
      "dist/",
      "build/"
    ]
  }